datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Mytable {

  Personid         Int   @id @default(autoincrement())
  LastName     String  
  FirstName    String  
  Age             Int

}

model City {
  ID          Int     @id
  Name        String
  CountryCode String
  District    String
  Population  Int
}