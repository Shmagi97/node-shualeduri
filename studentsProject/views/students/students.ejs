<!DOCTYPE html>
<html lang="en">
  <head>
    <link
      rel="stylesheet"
      type="text/css"
      href="/styles/students/students.css"
    />
    <!-- <link rel="stylesheet" href="/styles/students/students.css"> -->
  </head>
  <body class="container">
    <!-- header ejs -->
    <%- include('../../views/header/header.ejs'); %>
  
    <main>
      <div class="firstDiv">

        <h2>User Information</h2>
        <table>
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Age</th>
                    
                </tr>
            </thead>
            <tbody>
  
                 <% if ( typeof users === 'object' && users.length > 0 ) { %>
                  <% users.map((el)=> { %>
                    <tr>
                      <td> <%= el.firstName %> </td>
                      <td> <%= el.lastName %> </td>
                      <td> <%= el.age %> </td>
                    </tr>
                  <% }) %>

                  <% } else { %>
                      <h1 class="dataH1Info">The data is empty</h1>
                 <% } %>
                
            </tbody>
        </table>
      
      </div>
      <a href="/addStudents">
        <button class="register"> Registration Students</button>
      </a>
    </main>

    <footer></footer>
  </body>
</html>

